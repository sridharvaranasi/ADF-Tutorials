{
	"name": "DS_BlobStorage",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_BlobStorage",
			"type": "LinkedServiceReference"
		},
		"type": "AzureBlob",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": {
				"value": "concat('sales',@item().strartdate,'.csv')",
				"type": "Expression"
			},
			"folderPath": "data"
		}
	}
}